{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "apis/*"
  ],
  "scripts": {
    "build": "dotenv -e .env -- turbo run build",
    "clean": "turbo run clean",
    "caddy": "caddy run --config Caddyfile.dev",
    "dev": "dotenv -e .env.dev -- turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "dotenv -e .env.test -- turbo run test"
  },
  "dependencies": {},
  "devDependencies": {
    "@repo/eslint-config": "*",
    "prettier": "^3.1.1",
    "turbo": "latest",
    "dotenv-cli": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
